  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/puppeteer": "^7.0.4",
    "@vitest/ui": "^4.0.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
